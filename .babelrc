{
    "presets": [
      ["env", { "modules": false }],
      ["latest", {
        "es2015": {
          "modules": false
        }
      }],
      "stage-3"
    ],
    "plugins": [
      ["transform-vue-jsx", { "jsx": "this.$h" }],
      [
        "module-resolver",
        {
          "root": ["src/"]
        }
      ]
    ]
  }